            class="cn.com.wei.view.Cashier"
            icon="src/img/CashierMini.png"
            id="cn.com.wei.view.Cashier"
            name="收银台">
      </view>
      <view
            class="cn.com.wei.view.Manager"
            icon="src/img/managerMini.png"
            id="cn.com.wei.view.Manager"
            name="管理员">
      </view>
      <view
            class="cn.com.wei.view.Boss"
            id="cn.com.wei.view.Boss"
            name="经理">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="cn.com.wei.editor.Jiezhang"
            icon="src/img/22.png"
            id="cn.com.wei.editor.Jiezhang"
            name="结账">
